@font-face {
    font-family: 'NotoSans';
    src: url(http://fonts.googleapis.com/css?family=Noto+Sans);
    src: url('<%= asset_path('NotoSans-Regular.eot') %>'+'?#iefix') format('embedded-opentype'),
         url('<%= asset_path('fonts/arimo-regular-webfont.woff') %>') format('woff'),
         url('<%= asset_path('NotoSans-Regular.ttf') %>') format('truetype'),
         url('<%= asset_path('NotoSans-Regular.svg') %>#ArimoRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}